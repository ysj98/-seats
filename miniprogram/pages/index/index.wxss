.box {
  width: 100%;
  margin: 0 auto;
  overflow: hidden;
}

.animate {
  padding-left: 20px;
  font-size: 27rpx;
  font-weight: 600;
  color: #88888b;
  display: inline-block;
  white-space: nowrap;
  animation: 10s wordsLoop linear infinite normal;
  line-height: 80rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  letter-spacing: 2px;
  text-align: center;
  
}

@keyframes wordsLoop {
  0% {
      transform: translateX(100%);
      -webkit-transform: translateX(100%);
  }
  100% {
      transform: translateX(-100%);
      -webkit-transform: translateX(-100%);
  }
}

@-webkit-keyframes wordsLoop {
  0% {
      transform: translateX(100%);
      -webkit-transform: translateX(100%);
  }
  100% {
      transform: translateX(-100%);
      -webkit-transform: translateX(-100%);
  }
}
